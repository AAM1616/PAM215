const personas = [
    { nombre: "Ana", edad: 22},
    { nombre: "Luis", edad: 35},
    { nombre: "Maria", edad: 28},
];

const foundLuis = personas.find(p => p.nombre === "Luis");
console.log("Persona encontrada:", foundLuis);

personas.forEach(p=> {
  console.log( +p.nombre+ " tiene " +p.edad+ " aÃ±os");
});

const totalEdades = personas.reduce((acum, p) => acum + p.edad, 0);
console.log( "Suma total de edades:", totalEdades);